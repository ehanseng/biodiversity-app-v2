{
  "name": "biodiversity-app-v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web --output-dir dist",
    "build:web": "expo export --platform web --output-dir dist",
    "build:prod": "cross-env EXPO_PUBLIC_ENVIRONMENT=production expo export --platform web --output-dir dist",
    "build:prod:win": "set EXPO_PUBLIC_ENVIRONMENT=production && expo export --platform web --output-dir dist",
    "build:simple": "expo export --platform web --output-dir dist --clear",
    "preview": "npx serve dist -s",
    "serve": "npx serve dist -s -p 3000"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "expo": "~54.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-location": "~19.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "serve": "^14.2.1"
  },
  "private": true
}
